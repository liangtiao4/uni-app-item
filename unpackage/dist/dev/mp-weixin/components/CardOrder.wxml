<view data-event-opts="{{[['tap',[['toDetail',['$event']]]]]}}" class="card" bindtap="__e"><view class="card-top"><view class="column-center"><image class="img-avatar ia-ssm" src="{{o.seller_avatar}}"></image><text class="fs-3">{{o.seller_nickname}}</text></view><view class="text-oringin row-hidden-2">{{o.state_desc}}</view></view><view class="card-middle"><view><image class="img mr-2" src="{{o.img}}"></image></view><view class="card-desc"><view class="card-desc-info"><text class="mb-1">{{o.title}}</text><text class="text-desc">{{o.content}}</text></view><view class="card-desc-price"><price-format class="mb-1" vue-id="561c21da-1" price="{{o.price}}" bind:__l="__l"></price-format><text class="text-desc">{{"x"+o.count}}</text></view></view></view><view class="total-price"><price-format class="mr-2" vue-id="561c21da-2" title="优惠" price="{{o.discount}}" color="grey" bind:__l="__l"></price-format><price-format vue-id="561c21da-3" title="实付款" price="{{o.price-o.discount}}" bind:__l="__l"></price-format></view><view class="card-bottom"><view data-event-opts="{{[['tap',[['handleMore',['$event']]]]]}}" class="text-desc fs-2" bindtap="__e">更多</view><slot></slot></view></view>