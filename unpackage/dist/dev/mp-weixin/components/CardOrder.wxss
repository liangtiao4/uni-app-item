/* flex常用布局 */
.d-flex, .card-bottom > .card-btns {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.row-between, .card-top, .card-bottom {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.column-between {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.column-center, .card-middle {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.center-column {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.flex-grow-1 {
  -webkit-box-flex: 1;
  -webkit-flex-grow: 1;
          flex-grow: 1;
}
/* 百分比、宽高 */
.h-180 {
  height: 180rpx;
}
.h-120 {
  height: 120rpx;
}
/* 间隔 */
.ml-2 {
  margin-left: 20rpx;
}
.mr-1 {
  margin-right: 10rpx;
}
.mr-2 {
  margin-right: 20rpx;
}
.mb-1, .card-top {
  margin-bottom: 10rpx;
}
.mb-2 {
  margin-bottom: 20rpx;
}
.mt-1, .card-bottom {
  margin-top: 12rpx;
}
.wrap-p {
  padding: 12rpx 30rpx;
}
.p-sm {
  padding: 12rpx;
}
/* 定位 */
.fixed-b {
  position: fixed;
  bottom: 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  width: calc(100% - 20px);
  padding: 12rpx 24rpx;
  background-color: #fff;
  border-top: 1rpx solid #edecf0;
  box-shadow: 0 0 0 5rpx #eeedf1;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  font-size: 34rpx;
}
/* 按钮 */
.btn {
  width: 80px;
  height: 30px;
  text-align: center;
  line-height: 30px;
  border-radius: 40px;
  padding: 6rpx;
  font-size: 34rpx;
  color: #FFFFFF;
}
.btn-submit {
  background-color: #ff4925;
}
.btn-left {
  border-radius: 40rpx 0 0 40rpx;
  background-color: #ff8917;
  padding-left: 24rpx;
}
.btn-right {
  border-radius: 0 40rpx 40rpx 0;
  background-color: #ff4925;
  padding-right: 24rpx;
}
.btn-default {
  width: 75px;
  height: 24px;
  line-height: 24px;
  font-size: 34rpx;
  color: #333333;
  border: 1rpx solid #edecf0;
}
/* 图片 */
.img {
  width: 180rpx;
  height: 180rpx;
  border-radius: 10rpx;
  border: 1rpx solid #edecf0;
}
.img-goods {
  width: 100%;
}
.img-avatar {
  border-radius: 50%;
  border: 1rpx solid #edecf0;
  margin-right: 12rpx;
}
.ia-normal {
  width: 140rpx;
  height: 140rpx;
  border-radius: 30rpx;
}
.ia-sm {
  width: 85rpx;
  height: 85rpx;
}
.ia-ssm {
  width: 50rpx;
  height: 50rpx;
}
.card {
  font-size: 34rpx;
  border: 1rpx solid #edecf0;
  border-radius: 10rpx;
  box-shadow: 0 0 0 1 #EEEEEE;
  background-color: #fff;
  margin-bottom: 24rpx;
  padding: 12rpx 24rpx;
}
.card .total-price {
  text-align: right;
}
.card-bottom {
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.card-bottom > .card-btns view {
  margin-left: 24rpx;
}

