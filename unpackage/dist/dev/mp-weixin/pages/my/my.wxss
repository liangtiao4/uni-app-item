/* 定位 */
.fixed-b {
  position: fixed;
  bottom: 0;
  width: calc(100% - 20px);
  padding: 12rpx 24rpx;
  background-color: #fff;
  border-top: 1rpx solid #edecf0;
  box-shadow: 0 0 0 5rpx #eeedf1;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
