          justify-content: space-between;
  font-size: 30rpx;
}
/* 按钮 */
.btn {
  width: 80px;
  height: 30px;
  text-align: center;
  line-height: 30px;
  border-radius: 40px;
  padding: 6rpx;
  font-size: 30rpx;
  color: #FFFFFF;
}
.btn-submit {
  background-color: #ff4925;
}
.btn-left {
  border-radius: 40rpx 0 0 40rpx;
  background-color: #ff8917;
  padding-left: 24rpx;
}
.btn-right {
  border-radius: 0 40rpx 40rpx 0;
  background-color: #ff4925;
  padding-right: 24rpx;
}
/* 图片 */
.img {
  width: 180rpx;
  height: 180rpx;
  border-radius: 10rpx;
  border: 1rpx solid #edecf0;
}
.img-goods {
  width: 100%;
}
.img-avatar-lg {
  width: 140rpx;
  height: 140rpx;
  border-radius: 30rpx;
  border: 1rpx solid #edecf0;
}
.img-avatar-sm {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  border: 1rpx solid #edecf0;
}
